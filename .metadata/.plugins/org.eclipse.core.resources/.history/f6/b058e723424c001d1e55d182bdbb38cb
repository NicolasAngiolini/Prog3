import java.util.ArrayList;
import java.util.LinkedList;
import java.util.List;


public class ServicioBFS {

	private Grafo<?> grafo;
	
	public ServicioBFS(Grafo<?> grafo) {
		this.grafo = grafo;
	}
	
	public List<Integer> bfsForest(Vertice v) {
		LinkedList<<T>> lista = new LinkedList<Integer>();		//queue where new un-visited nodes are stored
		lista.addAll(v);
		
	}

	
}


